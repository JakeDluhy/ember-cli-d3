{{#block-page visual=model}}
  {{#data-visual as |ctx width height|}}
    {{#cart-lines select=(transition ctx.svg.select.chart) model=dimensionalData width=width height=height
          margin="10 30 25 65" stroke=(color-scale "category10") onhover=(action "track")
        as |selection xScale yScale width height|}}

      {{cart-tracker select=ctx.svg.select.chart.tracker model=tracked
        xScale=xScale yScale=yScale width=width height=height
      }}

      {{cart-axis select=(transition ctx.svg.select.chart.x-axis) scale=xScale
        orient="bottom" tickSize=(negative height) ticks=8 tickPadding=6
      }}

      {{cart-axis select=(transition ctx.svg.select.chart.y-axis) scale=yScale
        orient="left" tickSize=(negative width) tickPadding=6
      }}

    {{/cart-lines}}
  {{/data-visual}}
{{/block-page}}
