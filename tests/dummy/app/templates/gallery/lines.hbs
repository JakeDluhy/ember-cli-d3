{{#block-page visual=model}}
  {{#data-visual as |svg width height|}}
    {{#cart-lines select=(transition svg.chart) model=dimensionalData width=width height=height
          margin="10 30 25 65" stroke=(color-scale "category10")
        as |selection xScale yScale width height selected|}}

      {{cart-tracker select=svg.chart.tracker model=selected
        xScale=xScale yScale=yScale width=width height=height
      }}

      {{cart-axis select=(transition svg.chart.x-axis) scale=xScale
        orient="bottom" tickSize=(negative height) ticks=8 tickPadding=6
      }}
      {{cart-axis select=(transition svg.chart.y-axis) scale=yScale
        orient="left" tickSize=(negative width) tickPadding=6
      }}

    {{/cart-lines}}
  {{/data-visual}}
{{/block-page}}
